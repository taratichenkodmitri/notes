{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "templates/component/TemplateName.jsx",
        "style": "templates/component/TemplateName.css"
      }
    },
    "layout": {
      "path": "src/layout",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "customTemplates": {
        "component": "templates/component/TemplateName.jsx",
        "style": "templates/component/TemplateName.css"
      }
    }
  }
}
